#include <iostream>

using namespace std;

int main()
{
 int n,i,j,terkecil;
 cin >> n ;
 int baris[n],baris_baru[n];
 for ( i = 0 ; i < n ; i++) {
 cin >> baris[i];
 }
 for ( i = 0; i < n ; i++) {
    terkecil = baris[i];
    for ( j = i+1 ; j < n ; j++) {
        if (terkecil > baris[j]) {
            terkecil = baris[j];
            baris[j]= baris[i];
            baris[i]= terkecil;
        }
    }
 }
  j = 0;
 for ( i = 0 ; i < n ; i++) {
    if (i == 0 ) {
        baris_baru[j] = baris[i];
        j += 1;
        continue;
    }
    if (baris[i] == baris_baru[j-1]) {
        continue;
    }
    else {
        baris_baru[j] = baris[i];
        j += 1;
    }
 }
 cout << endl;
 for ( i = 0 ; i < j ; i++) {
    if ( i == 0) {
        cout << baris_baru[i];
        continue;
    }
    if ( baris_baru[i]- baris_baru[i-1] == 1){
        continue;
    }
    else {
        if ( baris_baru[i-1] - baris_baru[i-2] == 1 ) {
            cout << "-" << baris_baru[i-1] << "," << baris_baru[i];
        }
        else if ( i == j-1) {
            cout << "-" << baris_baru[i];
        }
        else {
            cout << "," << baris_baru[i];
        }
    }
 }
  return 0;
}
